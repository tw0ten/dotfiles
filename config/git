[init]
	defaultBranch = main
[alias]
	a = add
	aa = a --all
	c = commit
	ca = c --amend
	g = clone
	gd = g --depth 1
	d = diff
	dh = d HEAD
	do = d origin
	r = restore
	rs = r --staged
	s = status
	sw = switch
	cf = config
	cfl = cf --list
	l = log --decorate --numstat --graph
[user]
	name = tw0ten
	email = twoten.eq@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = false
